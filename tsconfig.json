{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020"],
    "types": ["cypress", "node"],
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["cypress/*"]
    }
  },
  "include": ["cypress/**/*.ts"]
}
